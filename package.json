    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.6.1",
    "pg-hstore": "^2.3.2",
    "prettier": "^1.14.0",
    "pretty-quick": "^1.6.0",
    "protractor": "~5.4.0",
    "puppeteer": "^1.4.0",
    "rxjs": "~6.3.3",
    "stylelint": "~9.6.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recommended-scss": "~3.2.0",
    "stylelint-config-standard": "~18.2.0",
    "stylelint-scss": "~3.3.0",
    "ts-node": "~7.0.1",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "~3.1.3",
    "winston": "^3.1.0",
    "zone.js": "^0.8.26"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  }
}
